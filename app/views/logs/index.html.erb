<h1>Here are your logs:</h1>

  <div class="padding_bottom">
    <table class="table table-striped table-bordered">
      <thead>
        <tr class="blue_white_table_header">
          <th></th>
          <th>Log Name</th>
        </tr>
      </thead>
      <tbody>
        <% @logs.each do |mylog| %>
          <tr>
            <td class="center_text" width="80">
              <span class="padding_right_10"><a href="/"><i class="fa fa-trash-o text_color_black"></i></a></span>
              <span><a href="/"><i class="fa fa-pencil text_color_black"></i></a></span>
            </td>
            <td><%= link_to mylog.log_name, log_expenses_path(mylog.id), method: :get %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>

